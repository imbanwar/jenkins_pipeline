pipeline {
  agent any
  parameters{
    string defaultValue: "100", description: "choose your value1", value1: "value1"
    string defaultValue: "200", description: "choose your value2", value2: "value2"
    }
  stages {
    stage('version') {
      steps {
        bat 'python --version'
      }
    }
    stage('hello') {
      steps {
        bat 'python parameter_add.py' ${value1} ${value2}
      }
    }
  }
}
